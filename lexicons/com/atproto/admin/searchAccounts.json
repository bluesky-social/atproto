{
  "lexicon": 1,
  "id": "com.atproto.admin.searchAccounts",
  "defs": {
    "main": {
      "type": "query",
      "description": "Get list of accounts that matches your search query.",
      "parameters": {
        "type": "params",
        "properties": {
          "email": { "type": "string" },
          "cursor": { "type": "string" }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["accounts"],
          "properties": {
            "cursor": { "type": "string" },
            "accounts": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "#accountSearchResult"
              }
            }
          }
        }
      }
    },
    "accountSearchResult": {
      "type": "object",
      "required": ["did", "email"],
      "properties": {
        "did": { "type": "string" },
        "email": { "type": "string" },
        "normalizedEmail": { "type": "string" },
        "handle": { "type": "string" }
      }
    }
  }
}
