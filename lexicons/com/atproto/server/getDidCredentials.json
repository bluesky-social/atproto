{
  "lexicon": 1,
  "id": "com.atproto.server.getDidCredentials",
  "defs": {
    "main": {
      "type": "query",
      "description": "Describe the credentials that should be included in the DID doc of an account that is migrating to this service.",
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["signingKey"],
          "properties": {
            "signingKey": {
              "type": "string"
            },
            "rotationKeys": {
              "type": "array",
              "items": { "type": "string" }
            },
            "pdsEndpoint": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
