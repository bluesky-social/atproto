{
  "lexicon": 1,
  "id": "com.atproto.sync.subscribeAllRepos",
  "defs": {
    "main": {
      "type": "subscription",
      "description": "Subscribe to repo updates",
      "parameters": {
        "type": "params",
        "properties": {
          "cursor": {
            "type": "integer",
            "description": "The last known event to backfill from."
          }
        }
      },
      "message": {
        "schema": {
          "type": "object",
          "required": ["seq", "event", "repo", "commit", "prev", "blocks", "blobs", "time"],
          "nullable": ["prev"],
          "properties": {
            "seq": {"type": "integer"},
            "event": { 
              "type": "string",
              "knownValues": [
                "repo_append",
                "rebase"
              ]
            },
            "repo": {"type": "string"},
            "commit": {"type": "string"},
            "prev": {"type": "string"},
            "blocks": {"type": "unknown"},
            "blobs": {
              "type": "array",
              "items": {"type": "string"}
            },
            "time": {"type": "datetime"}
          }
        }
      },
      "infos": [
        {"name": "OutdatedCursor"}
      ],
      "errors": [
        {"name": "FutureCursor"}
      ]
    }
  }
}