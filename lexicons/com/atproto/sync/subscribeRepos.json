{
  "lexicon": 1,
  "id": "com.atproto.sync.subscribeRepos",
  "defs": {
    "main": {
      "type": "subscription",
      "description": "Subscribe to repo updates",
      "parameters": {
        "type": "params",
        "required": ["dids", "lastSeen"],
        "properties": {
          "dids": { 
            "type": "array", 
            "items": {"type": "string"}
          },
          "lastSeen": {"type": "number", "description": "The sequence number of the last seen repo update."}
        }
      },
      "output": {
        "encoding": "application/vnd.ipld.car"
      }
    }
  }
}