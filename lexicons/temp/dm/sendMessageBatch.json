{
  "lexicon": 1,
  "id": "temp.dm.sendMessageBatch",
  "defs": {
    "main": {
      "type": "procedure",
      "input": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["items"],
          "properties": {
            "items": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "#batchItem"
              }
            }
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["items"],
          "properties": {
            "items": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "temp.dm.defs#messageView"
              }
            }
          }
        }
      }
    },
    "batchItem": {
      "type": "object",
      "required": ["chatId", "message"],
      "properties": {
        "chatId": { "type": "string" },
        "message": {
          "type": "ref",
          "ref": "temp.dm.defs#message"
        }
      }
    }
  }
}
