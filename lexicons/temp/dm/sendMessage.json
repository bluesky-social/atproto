        "schema": {
          "type": "object",
          "required": ["chatId", "message"],
          "properties": {
            "chatId": { "type": "string" },
            "message": {
              "type": "ref",
              "ref": "temp.dm.defs#message"
            }
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["messageId"],
          "properties": {
            "messageId": { "type": "string" }
          }
        }
      }
    }
  }
}
