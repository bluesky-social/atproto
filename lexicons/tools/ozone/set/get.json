{
  "lexicon": 1,
  "id": "tools.ozone.sets.get",
  "defs": {
    "main": {
      "type": "query",
      "description": "Get a specific set and its values",
      "parameters": {
        "type": "params",
        "required": ["name"],
        "properties": {
          "name": {
            "type": "string"
          },
          "limit": {
            "type": "integer",
            "minimum": 1,
            "maximum": 1000,
            "default": 100
          },
          "cursor": {
            "type": "string"
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["set", "values"],
          "properties": {
            "set": {
              "type": "ref",
              "ref": "tools.ozone.sets.defs#setView"
            },
            "values": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "cursor": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}
