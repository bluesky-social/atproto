{
  "lexicon": 1,
  "id": "tools.ozone.set.removeSet",
  "defs": {
    "main": {
      "type": "procedure",
      "description": "Remove an entire set. Attempting to remove a set that does not exist will result in an error.",
      "input": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["name"],
          "properties": {
            "name": {
              "type": "string",
              "description": "Name of the set to remove"
            }
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "properties": {}
        }
      },
      "errors": [
        {
          "name": "SetNotFound",
          "description": "set with the given name does not exist"
        }
      ]
    }
  }
}
