{
  "lexicon": 1,
  "id": "app.bsky.notification.defs",
  "defs": {
    "recordDeleted": {
      "type": "object",
      "properties": {}
    },
    "channels": {
      "type": "object",
      "required": ["list", "push"],
      "properties": {
        "list": { "type": "boolean", "default": true },
        "push": { "type": "boolean", "default": true }
      }
    },
    "channelsPush": {
      "type": "object",
      "required": ["push"],
      "properties": {
        "push": { "type": "boolean", "default": true }
      }
    },
    "preferenceFull": {
      "type": "object",
      "required": ["channels", "filter"],
      "properties": {
        "channels": {
          "type": "ref",
          "ref": "#channels"
        },
        "filter": {
          "type": "string",
          "knownValues": ["all", "follows"],
          "default": "all"
        }
      }
    },
    "preferenceNoFilter": {
      "type": "object",
      "required": ["channels"],
      "properties": {
        "channels": {
          "type": "ref",
          "ref": "#channels"
        }
      }
    },
    "preferencePush": {
      "type": "object",
      "required": ["channels", "filter"],
      "properties": {
        "channels": {
          "type": "ref",
          "ref": "#channelsPush"
        },
        "filter": {
          "type": "string",
          "knownValues": ["all", "follows"],
          "default": "all"
        }
      }
    },
    "preferences": {
      "type": "object",
      "required": [
        "chat",
        "follow",
        "like",
        "likeViaRepost",
        "mention",
        "quote",
        "reply",
        "repost",
        "repostViaRepost",
        "starterpackJoined",
        "subscribedPost",
        "unverified",
        "verified"
      ],
      "properties": {
        "chat": { "type": "ref", "ref": "#preferencePush" },
        "follow": { "type": "ref", "ref": "#preferenceFull" },
        "like": { "type": "ref", "ref": "#preferenceFull" },
        "likeViaRepost": { "type": "ref", "ref": "#preferenceFull" },
        "mention": { "type": "ref", "ref": "#preferenceFull" },
        "quote": { "type": "ref", "ref": "#preferenceFull" },
        "reply": { "type": "ref", "ref": "#preferenceFull" },
        "repost": { "type": "ref", "ref": "#preferenceFull" },
        "repostViaRepost": { "type": "ref", "ref": "#preferenceFull" },
        "starterpackJoined": { "type": "ref", "ref": "#preferenceNoFilter" },
        "subscribedPost": { "type": "ref", "ref": "#preferenceNoFilter" },
        "unverified": { "type": "ref", "ref": "#preferenceNoFilter" },
        "verified": { "type": "ref", "ref": "#preferenceNoFilter" }
      }
    }
  }
}
