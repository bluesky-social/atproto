{
  "lexicon": 1,
  "id": "app.bsky.bookmark.defs",
  "defs": {
    "bookmark": {
      "description": "Object used to represent a bookmark subject and to store bookmark data in stash.",
      "type": "object",
      "required": ["subject"],
      "properties": {
        "subject": {
          "description": "A strong ref to the record to be bookmarked. Currently, only `app.bsky.feed.post` records are supported.",
          "type": "ref",
          "ref": "com.atproto.repo.strongRef"
        }
      }
    },
    "bookmarkView": {
      "type": "object",
      "required": ["item", "bookmark"],
      "properties": {
        "bookmark": { "type": "ref", "ref": "#bookmark" },
        "createdAt": { "type": "string", "format": "datetime" },
        "item": {
          "type": "union",
          "refs": [
            "app.bsky.feed.defs#blockedPost",
            "app.bsky.feed.defs#notFoundPost",
            "app.bsky.feed.defs#postView"
          ]
        }
      }
    }
  }
}
