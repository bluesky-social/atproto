{
  "lexicon": 1,
  "id": "app.bsky.embed.images",
  "description": "A set of images embedded in some other form of content",
  "defs": {
    "main": {
      "type": "object",
      "required": ["images"],
      "properties": {
        "images": {
          "type": "array",
          "items": {"type": "ref", "ref": "#image"},
          "maxLength": 4
        }
      }
    },
    "image": {
      "type": "object",
      "required": ["image", "alt"],
      "properties": {
        "image": {
            "type": "image",
            "accept": ["image/*"],
            "maxWidth": 500,
            "maxHeight": 500,
            "maxSize": 300000
        },
        "alt": {"type": "string"}
      }
    }
  }
}