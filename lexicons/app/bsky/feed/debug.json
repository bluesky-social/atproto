{
  "lexicon": 1,
  "id": "app.bsky.feed.debug",
  "defs": {
    "main": {
      "type": "record",
      "key": "tid",
      "record": {
        "type": "object",
        "required": ["text", "createdAt"],
        "properties": {
          "text": {"type": "string", "maxLength": 256},
          "entities": {
            "type": "array",
            "items": "#entity"
          },
          "reply": "#reply",
          "createdAt": {"type": "string"}
        }
      }
    },
    "someToken": {"type": "token"},
    "unknown": {"type": "unknown"},
    "boolean": {"type": "boolean"},
    "number": {"type": "number"},
    "integer": {"type": "integer"},
    "string": {"type": "string"},
    "blob": {"type": "blob"},
    "image": {"type": "image"},
    "video": {"type": "video"},
    "audio": {"type": "audio"},
    "stringArr": {
      "type": "array",
      "items": {"type": "string"}
    },
    "replyArr": {
      "type": "array",
      "items": "#reply"
    },
    "multiArr": {
      "type": "array",
      "items": ["#reply", "#postRef", "#entity"]
    },
    "reply":{
      "type": "object",
      "required": ["root", "parent"],
      "properties": {
        "root": "#postRef",
        "parent": "#postRef"
      }
    },
    "postRef": {
      "type": "object",
      "required": ["uri", "cid"],
      "properties": {
        "uri": {"type": "string"},
        "cid": {"type": "string"}
      }
    },
    "entity": {
      "type": "object",
      "required": ["index", "type", "value"],
      "properties": {
        "index": "#textSlice",
        "type": {
          "type": "string",
          "description": "Expected values are 'mention', 'hashtag', and 'link'."
        },
        "value": {"type": "string"}
      }
    },
    "textSlice": {
      "type": "object",
      "required": ["start", "end"],
      "properties": {
        "start": {"type": "integer", "minimum": 0},
        "end": {"type": "integer", "minimum": 0}
      }
    }
  }
}
