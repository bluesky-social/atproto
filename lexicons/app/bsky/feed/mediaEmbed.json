{
  "lexicon": 1,
  "id": "app.bsky.feed.mediaEmbed",
  "defs": {
    "main": {
      "type": "object",
      "description": "A list of media embedded in a post or document.",
      "required": ["media"],
      "properties": {
        "media": {"type": "array", "items": "#mediaEmbed"}
      }
    },
    "mediaEmbed": {
      "type": "object",
      "required": ["original"],
      "properties": {
        "alt": {"type": "string"},
        "thumb": {"type": "image"},
        "original": {"type": "blob"}
      }
    }
  }
}