{
  "lexicon": 1,
  "id": "app.bsky.feed.embed",
  "description": "Content embedded in other content, such as an image or link embedded in a post.",
  "defs": {
    "main": {
      "type": "object",
      "description": "A list embeds in a post or document.",
      "required": ["media"],
      "properties": {
        "items": {
          "type": "array",
          "items": {
            "type": "union",
            "refs": ["#media", "#record", "#external", "#unknown"]
          }
        }
      }
    },
    "media": {
      "type": "object",
      "required": ["original"],
      "properties": {
        "alt": {"type": "string"},
        "thumb": {"type": "image"},
        "original": {"type": "blob"}
      }
    },
    "record": {
      "type": "object",
      "required": ["type", "author", "record"],
      "properties": {
        "type": {"type": "string", "const": "record"},
        "author": {"type": "ref", "ref": "app.bsky.actor.ref#withInfo"},
        "record": {"type": "unknown"}
      }
    },
    "external": {
      "type": "object",
      "required": ["type", "uri", "title", "description", "imageUri"],
      "properties": {
        "type": {"type": "string", "const": "external"},
        "uri": {"type": "string"},
        "title": {"type": "string"},
        "description": {"type": "string"},
        "imageUri": {"type": "string"}
      }
    },
    "unknown": {
      "type": "object",
      "required": ["type"],
      "properties": {
        "type": {"type": "string"}
      }
    }
  }
}