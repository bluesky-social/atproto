{
  "lexicon": 1,
  "id": "app.bsky.admin.takeModerationAction",
  "defs": {
    "main": {
      "type": "procedure",
      "description": "Take a moderation action on an actor.",
      "input": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["action", "subject", "reason", "createdBy"],
          "properties": {
            "action": {"type": "string", "knownValues": ["app.bsky.admin.moderationAction#takedown"]},
            "subject": {"type": "union", "refs": ["app.bsky.admin.moderationAction#subjectActor"]},
            "reason": {"type": "string"},
            "createdBy": {"type": "string"}
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "ref",
          "ref": "app.bsky.admin.moderationAction#view"
        }
      }
    }
  }
}
