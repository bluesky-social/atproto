{
  "lexicon": 1,
  "id": "app.bsky.graph.getList",
  "defs": {
    "main": {
      "type": "query",
      "description": "Fetch a list of actors",
      "parameters": {
        "type": "params",
        "required": ["list"],
        "properties": {
          "list": { "type": "string", "format": "at-uri" },
          "limit": {
            "type": "integer",
            "minimum": 1,
            "maximum": 100,
            "default": 50
          },
          "cursor": { "type": "string" }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["list", "items"],
          "properties": {
            "cursor": { "type": "string" },
            "list": { "type": "ref", "ref": "app.bsky.graph.defs#listView" },
            "items": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "app.bsky.graph.defs#listItemView"
              }
            }
          }
        }
      }
    }
  }
}
