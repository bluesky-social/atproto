{
  "lexicon": 1,
  "id": "app.bsky.graph.follow",
  "defs": {
    "main": {
      "type": "record",
      "description": "A social follow.",
      "key": "tid",
      "record": {
        "type": "object",
        "required": ["subject", "createdAt"],
        "properties": {
          "subject": {"type": "ref", "ref": "#subject"},
          "createdAt": {"type": "datetime"}
        }
      }
    },
    "subject": {
      "type": "object",
      "required": ["did", "declarationCid"],
      "properties": {
        "did": {"type": "string"},
        "declarationCid": {"type": "string"}
      }
    }
  }
}
