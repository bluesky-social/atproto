{
  "lexicon": 1,
  "id": "app.bsky.unspecced.getPopular",
  "defs": {
    "main": {
      "type": "query",
      "description": "An unspecced view of globally popular items",
      "parameters": {
        "type": "params",
        "properties": {
          "includeNsfw": { "type": "boolean", "default": false },
          "limit": {
            "type": "integer",
            "minimum": 1,
            "maximum": 100,
            "default": 50
          },
          "cursor": { "type": "string" }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": ["feed"],
          "properties": {
            "cursor": { "type": "string" },
            "feed": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "app.bsky.feed.defs#feedViewPost"
              }
            }
          }
        }
      }
    }
  }
}
