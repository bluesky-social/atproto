        "schema": {
          "type": "object",
          "required": ["labels"],
          "properties": {
            "labels": {
              "type": "array",
              "items": { "type": "ref", "ref": "com.atproto.label.defs#label" }
            }
          }
        }
      }
    }
  }
}
