{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": ["./browser.json"],
  "compilerOptions": {
    "jsx": "react-native",
    "target": "ES2023",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "moduleDetection": "force",
    "esModuleInterop": true,

    "skipLibCheck": true
  }
}
