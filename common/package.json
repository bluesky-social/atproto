{
  "name": "@bluesky/common",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "tsc -w --project tsconfig.json",
    "test": "ava -v test/*.ts",
    "lint": "eslint --ext .ts ."
  },
  "license": "MIT",
  "dependencies": {
    "@ipld/car": "^3.2.3",
    "@ipld/dag-cbor": "^7.0.0",
    "axios": "^0.24.0",
    "ipld-hashmap": "^2.1.10",
    "level": "^7.0.1",
    "multiformats": "^9.6.4",
    "ucans": "0.9.0-alpha3"
  },
  "devDependencies": {
    "@types/level": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "ava": "^3.15.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nonSemVerExperiments": {
      "configurableModuleFormat": true
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
