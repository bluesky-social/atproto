{
  "name": "@atproto-labs/xrpc-utils",
  "version": "0.0.3",
  "license": "MIT",
  "description": "XRPC server utilities for Node.JS",
  "keywords": [
    "atproto",
    "node",
    "xrpc",
    "server",
    "utilities",
    "content",
    "negotiation"
  ],
  "homepage": "https://atproto.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluesky-social/atproto",
    "directory": "packages/internal/xrpc-utils"
  },
  "type": "commonjs",
  "main": "dist/index.js",
