      "default": "./dist/accept.js"
    }
  },
  "dependencies": {
    "@atproto/xrpc": "workspace:^",
    "@atproto/xrpc-server": "workspace:^"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json"
