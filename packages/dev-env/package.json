    "@atproto/crypto": "*",
    "@atproto/did-resolver": "*",
    "@atproto/server": "*",
    "@atproto/uri": "*",
    "chalk": "^5.0.1",
    "dotenv": "^16.0.1",
    "get-port": "^6.1.2"
  },
  "devDependencies": {
    "esbuild-plugin-copy": "^1.3.0",
    "ts-node": "^10.8.1"
  }
}
