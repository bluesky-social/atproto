{
  "name": "@atproto/lex-router",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Request router for Atproto Lexicon protocols and schemas",
  "keywords": [
    "atproto",
    "lexicon",
    "router",
    "typescript"
  ],
  "homepage": "https://atproto.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluesky-social/atproto",
    "directory": "packages/lex/lex-router"
  },
  "files": [
    "./src",
    "./tsconfig.build.json",
    "./tsconfig.tests.json",
    "./tsconfig.json",
    "./dist",
    "./nodejs.js",
    "./CHANGELOG.md"
  ],
  "sideEffects": false,
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./nodejs": {
      "import": "./dist/nodejs.js",
      "require": "./dist/nodejs.js",
      "types": "./dist/nodejs.d.ts"
    }
  },
  "dependencies": {
    "@atproto/lex-schema": "workspace:*",
    "@atproto/lex-client": "workspace:*",
    "@atproto/lex-json": "workspace:*",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "jest": "^28.1.2"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "test": "jest"
  }
}
