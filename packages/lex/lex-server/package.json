{
  "name": "@atproto/lex-server",
  "version": "0.0.7",
  "license": "MIT",
  "description": "Request router for Atproto Lexicon protocols and schemas",
  "keywords": [
    "atproto",
    "lexicon",
    "router",
    "typescript"
  ],
  "homepage": "https://atproto.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluesky-social/atproto",
    "directory": "packages/lex/lex-server"
  },
  "files": [
    "./src",
    "./tsconfig.build.json",
    "./tsconfig.tests.json",
    "./tsconfig.json",
    "./dist",
    "./nodejs.js",
    "./CHANGELOG.md"
  ],
  "sideEffects": false,
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./nodejs": {
      "types": "./dist/nodejs.d.ts",
      "import": "./dist/nodejs.js",
      "require": "./dist/nodejs.js"
    }
  },
  "dependencies": {
    "@atproto-labs/did-resolver": "workspace:^",
    "@atproto/crypto": "workspace:^",
    "@atproto/did": "workspace:^",
    "@atproto/lex-cbor": "workspace:^",
    "@atproto/lex-data": "workspace:^",
    "@atproto/lex-json": "workspace:^",
    "@atproto/lex-schema": "workspace:^",
    "http-terminator": "^3.2.0",
    "tslib": "^2.8.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@atproto/lex": "workspace:^",
    "@atproto/lex-client": "workspace:^",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "4.0.16",
    "vitest": "^4.0.16"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "test": "vitest run"
  }
}
