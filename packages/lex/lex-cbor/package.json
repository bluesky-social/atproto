{
  "name": "@atproto/lex-cbor",
  "version": "0.0.3",
  "license": "MIT",
  "description": "Lexicon encoding utilities for AT Lexicon data in CBOR format",
  "keywords": [
    "atproto",
    "lex",
    "data",
    "cbor",
    "encoding",
    "utilities"
  ],
  "homepage": "https://atproto.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluesky-social/atproto",
    "directory": "packages/lex/lex-cbor"
  },
  "files": [
    "./src",
    "./tsconfig.build.json",
    "./tsconfig.tests.json",
    "./tsconfig.json",
    "./dist",
    "./CHANGELOG.md"
  ],
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "dependencies": {
    "@atproto/lex-data": "workspace:*",
    "multiformats": "^9.9.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@atproto/lex-json": "workspace:*",
    "cborg": "^4.3.0",
    "jest": "^28.1.2",
    "vite": "^6.2.0"
  },
  "scripts": {
    "dev": "vite build --watch",
    "prebuild": "vite build --emptyOutDir",
    "build": "tsc --build tsconfig.build.json",
    "test": "jest"
  }
}
