{
  "name": "@atproto/oauth-client",
  "version": "0.0.1",
  "license": "MIT",
  "description": "OAuth client for ATPROTO PDS. This package serves as common base for environment-specific implementations (NodeJS, Browser, React-Native).",
  "keywords": [
    "atproto",
    "oauth",
    "client",
    "isomorphic"
  ],
  "homepage": "https://atproto.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluesky-social/atproto",
    "directory": "packages/oauth-client"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@atproto/b64": "workspace:*",
    "@atproto/simple-store": "workspace:*",
    "@atproto/simple-store-memory": "workspace:*",
    "@atproto/did": "workspace:*",
    "@atproto/fetch": "workspace:*",
    "@atproto/fetch-dpop": "workspace:*",
    "@atproto/handle-resolver": "workspace:*",
    "@atproto/identity-resolver": "workspace:*",
    "@atproto/jwk": "workspace:*",
    "@atproto/oauth-types": "workspace:*",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json"
  }
}
