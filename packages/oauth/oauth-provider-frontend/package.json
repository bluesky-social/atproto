{
  "name": "@atproto/oauth-provider-frontend",
  "version": "0.0.0",
  "license": "MIT",
  "homepage": "https://atproto.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluesky-social/atproto",
    "directory": "packages/oauth/oauth-provider-frontend"
  },
  "type": "commonjs",
  "main": "dist/lib.js",
  "types": "dist/lib.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib.d.ts",
      "default": "./dist/lib.js"
    }
  },
  "scripts": {
    "i18n:extract": "lingui extract --clean",
    "i18n:compile": "lingui compile --typescript",
    "i18n": "pnpm i18n:extract && pnpm i18n:compile",
    "prebuild:src": "pnpm run i18n",
    "build:src": "vite build -- ignore additional npm args",
    "dev:ui": "vite --port 5173",
    "dev:src": "vite build --watch",
    "dev:catalogs": "pnpm run i18n:extract --debounce 250 --watch > /dev/null",
    "dev:messages": "pnpm run i18n:compile --debounce 500 --watch"
  },
  "devDependencies": {
    "@atproto/syntax": "workspace:*",
    "@atproto-labs/fetch": "workspace:*",
    "@atproto-labs/rollup-plugin-bundle-manifest": "workspace:*",
    "@atproto/oauth-provider-api": "workspace:*",
    "@atproto/oauth-types": "workspace:*",
    "@lingui/cli": "^5.2.0",
    "@lingui/core": "^5.2.0",
    "@lingui/react": "^5.2.0",
    "@lingui/swc-plugin": "^5.4.0",
    "@lingui/vite-plugin": "^5.2.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-toast": "^1.2.6",
    "@tailwindcss/vite": "^4.0.14",
    "@tanstack/react-form": "^1.1.2",
    "@tanstack/react-query": "^5.69.0",
    "@tanstack/react-router": "^1.114.25",
    "@tanstack/react-router-devtools": "^1.114.25",
    "@tanstack/router-plugin": "^1.114.25",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "clsx": "^2.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.14",
    "vite": "^6.2.0",
    "zod": "^3.23.8"
  }
}
